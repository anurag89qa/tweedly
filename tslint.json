{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "no-console": [false],
    "arrow-parens": [false],
    "quotemark": ["single"],
    "interface-name": [false],
    "semicolon": [false],
    "member-ordering": false,
    "no-empty-interface": false,
    "no-unused-expression": false,
    "no-unused-variable": false,
    "no-empty": false
  },
  "rulesDirectory": []
}